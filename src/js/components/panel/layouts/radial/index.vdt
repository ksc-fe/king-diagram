import {Form, FormItem} from 'kpc/components/form';
import {Spinner} from 'kpc/components/spinner';

<t:parent>
    <b:body>
        <Form labelWidth="100">
            <FormItem label="层级间距：">
                <Spinner vertical min={{ 0 }} v-model="levelDistance" suffix="px" />
            </FormItem>
            <FormItem label="图形间距：">
                <Spinner vertical min={{ 0 }} v-model="nodeDistance" suffix="px" />
            </FormItem>
        </Form>
    </b:body>
</t:parent>
